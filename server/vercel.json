{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30,
        "memory": 3008
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "server.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "server.js",
      "methods": ["GET", "POST", "PATCH", "DELETE"],
      "headers": {
        "Cache-Control": "no-cache",
        "Connection": "keep-alive"
      }
    },
    {
      "src": "/health",
      "dest": "server.js"
    }
  ]
}
